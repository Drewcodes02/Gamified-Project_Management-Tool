<!DOCTYPE html>
<html lang="en">
<%- include('partials/_head.ejs') %>
  <body>
<%- include('partials/_header.ejs') %>
    <main role="main" class="container mt-4">
      <h1>Profile</h1>
      <div class="my-4">
        <h2>Username: <%= username %></h2>
        <h3>Points: <%= points %></h3>
        <h3>Achievements</h3>
        <ul>
          <% achievements.forEach(function(achievement) { %>
            <li><%= achievement %></li>
          <% }); %>
        </ul>
      </div>
    </main>
  </body>
<%- include('partials/_footer.ejs') %>
</html>